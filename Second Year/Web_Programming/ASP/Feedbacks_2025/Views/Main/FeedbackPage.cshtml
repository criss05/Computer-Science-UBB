@{
	Layout = null;
}
<html>
<head>
	<title>My channels</title>
	<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
	<script>
		$(document).ready(function(){
			$.get("/Main/GetBadWordsCount", {},
			function(data, status){
				$("#flags").html(data);
			})

			$.get("/Main/GetFeedbacks", {},
			function(data, status){
				$("#feedbacksDiv").html(data);
			})
		});
	</script>
</head>
<body>
	<p id="flags"></p>
	<h2>Feedbacks</h2>
	<div id="feedbacksDiv"></div>
</body>
</html>
